{
  "name": "tablebox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:app": "next dev",
    "dev:ssl": "npx -y local-ssl-proxy@2.0.5 --source 3001 --target 3000",
    "dev:db": "docker compose up db",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "drizzle-kit push:mysql",
    "cron": "node --require esbuild-register scripts/cronexec.ts",
    "db:migrate:generate-auto": "drizzle-kit generate:mysql",
    "db:migrate:generate-custom": "drizzle-kit generate:mysql --custom",
    "db:migrate:up": "node --require esbuild-register scripts/migrate.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.9",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@lexical/react": "^0.12.5",
    "@lexical/yjs": "^0.12.5",
    "@liveblocks/client": "^1.8.2",
    "@liveblocks/node": "^1.8.2",
    "@liveblocks/react": "^1.8.2",
    "@liveblocks/yjs": "^1.8.2",
    "@planetscale/database": "^1.11.0",
    "@slack/bolt": "^3.16.0",
    "@slack/webhook": "^7.0.1",
    "@tiptap/extension-collaboration": "^2.1.13",
    "@tiptap/extension-collaboration-cursor": "^2.1.13",
    "drizzle-orm": "^0.28.6",
    "lexical": "^0.12.5",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "novel": "^0.1.22",
    "openai": "^4.23.0",
    "react": "^18",
    "react-dom": "^18",
    "react-moveable": "^0.56.0",
    "server-only": "^0.0.1",
    "yjs": "^13.6.10"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.19.13",
    "esbuild": "^0.19.8",
    "esbuild-register": "^3.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "mysql2": "^3.6.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.4"
  }
}
